{
    "name": "ardabeyazoglu/php-retention",
    "description": "File retention policy computation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpRetention\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Arda Beyazoglu",
            "email": "arda@beyazoglu.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^3.5"
    },
    "scripts": {
        "test": "phpunit"
    }
}
